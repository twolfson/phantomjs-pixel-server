    "commander": "~2.0.0",
    "phantomjsify": "~0.1.0",
    "request": "~2.27.0",
    "get-pixels": "~1.0.1",
    "save-pixels": "~0.3.0",
    "ndarray": "~1.0.8",
    "function-signature": "~0.2.0",
    "function-to-string": "~0.2.0"
